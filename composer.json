{
    "name": "fantestic/cestmanager-bundle",
    "description": "Provides an integration of fantestic/cestmanager into an api-platform installation.",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Gerald Baumeister",
            "email": "gerald@fantestic.io",
            "homepage": "https://fantestic.io/"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.0",
        "fantestic/cest-manager": "dev-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Fantestic\\CestManagerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fantestic\\CestManagerBundle\\Tests\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fantestic/cestmanager.git"
        }
    ]
}
